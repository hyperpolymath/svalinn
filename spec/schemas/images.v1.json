{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://svalinn.dev/schemas/images.v1.json",
  "title": "Images Response",
  "type": "object",
  "required": ["images"],
  "properties": {
    "images": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name", "tag", "digest", "verified"],
        "properties": {
          "name": { "type": "string" },
          "tag": { "type": "string" },
          "digest": { "type": "string" },
          "verified": { "type": "boolean" }
        }
      }
    }
  }
}
